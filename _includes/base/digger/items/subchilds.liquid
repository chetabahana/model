{%- comment -%}
*
*  collect subitems
*
{%- endcomment -%}
{%- assign my_index_size = my_index | size -%}
{%- if my_index_size > 0 -%}
	{%- include tabs.liquid size=my_size -%}{{ my_tab }}"items": [
	{%- for index in my_index -%}
		{{- my_captures[index] }}{% unless forloop.last %},{% endunless -%}
	{%- endfor -%}
	{%- include tabs.liquid size=my_size -%}{{ my_tab }}]
{%- else -%}
	{%- include tabs.liquid size=my_size -%}{{ my_tab }}"items": []
{%- endif -%}
